def main():
    dictionary = 'Oxford English Dictionary.txt'
    dic = []
    print("hello world")
    with open(dictionary) as txtfile:
         for line in txtfile:
             if line != '\n':
                dic.append(line)

    count = 0
    with open("HangmanDictionary.txt",'w') as newtxtfile:
        for line in dic:
            count+=1
            newtxtfile.write(line)
    print(count,dic[count-1])

main()
